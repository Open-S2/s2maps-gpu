{
  "name": "s2maps-gpu",
  "version": "0.18.0",
  "description": "S2 Maps GPU",
  "keywords": [
    "s2",
    "s2-geometry",
    "maps",
    "cartography",
    "gpu",
    "webgpu",
    "webgl2",
    "webgl",
    "digital",
    "mapping",
    "engine"
  ],
  "homepage": "https://opens2.com/s2maps-gpu",
  "bugs": {
    "url": "https://github.com/Open-S2/s2maps-gpu/issues"
  },
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/Open-S2"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Open-S2/s2maps-gpu.git"
  },
  "scripts": {
    "build:dev": "nuxt build",
    "build:S2": "node ./configS2/build.js",
    "build:S2:flat": "bun run configS2/buildBunFlat.ts && node ./configS2/buildFlat.js",
    "build:ESM": "tsc --project tsconfig.json",
    "build:types": "tsc --project tsconfig.json --emitDeclarationOnly --declaration --declarationDir dist-types",
    "dev": "DEV=true nuxt dev --host --disable-telemetry",
    "dev:playwright": "nuxt dev --host --no-watch --no-lint --no-progress --disable-telemetry --no-source-map --quiet",
    "server:playwright": "bun run ./tests-playwright/server.ts",
    "lint:tsc": "tsc --noEmit --project tsconfig.eslint.json",
    "lint:S2": "eslint s2 --ext .ts",
    "test:S2": "bun test",
    "test:S2:watch": "bun test --watch",
    "test:playwright": "bun run build:S2:flat && playwright test -c ./tests-playwright/playwright.config.ts",
    "test:playwright:ui": "bun run build:S2:flat && playwright test --ui -c ./tests-playwright/playwright.config.ts",
    "prepare:toAdd": "bun run lint:S2 && bun run build:ESM && bun run build:types",
    "docs": "typedoc",
    "loc": "tokei ./s2 # && tokei ./rust"
  },
  "exports": {
    ".": "./dist/index.js",
    "./plugins": "./dist/plugins/index.js"
  },
  "main": "./dist/index.js",
  "types": "./dist-types/index.d.ts",
  "dependencies": {
    "earclip": "^1.1.1",
    "gis-tools": "file:../gis-tools",
    "open-vector-tile": "^1.11.0",
    "s2-pmtiles": "^1.1.1",
    "unicode-shaper": "0.6.2"
  },
  "devDependencies": {
    "@happy-dom/global-registrator": "^17.4.4",
    "@maplibre/maplibre-gl-style-spec": "^23.1.0",
    "@nuxt/eslint": "^1.3.0",
    "@playwright/experimental-ct-vue": "^1.51.1",
    "@playwright/test": "^1.51.1",
    "@rollup/pluginutils": "^5.1.4",
    "@types/better-sqlite3": "^7.6.13",
    "@types/bun": "^1.2.9",
    "@types/webpack": "^5.28.5",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "@vitejs/plugin-vue": "^5.2.3",
    "@webgpu/types": "^0.1.60",
    "ajv": "^8.17.1",
    "better-sqlite3": "^11.9.1",
    "compression-webpack-plugin": "^11.1.0",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "esbuild": "^0.25.2",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-jsdoc": "^50.6.9",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-svelte3": "^4.0.0",
    "eslint-plugin-vue": "^10.0.0",
    "filesize": "^10.1.6",
    "find-cache-dir": "^6.0.0",
    "mini-css-extract-plugin": "^2.9.2",
    "nuxt": "^3.16.2",
    "picocolors": "^1.1.1",
    "raw-loader": "^4.0.2",
    "s2-tilejson": "^1.12.0",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typedoc": "^0.28.2",
    "typedoc-plugin-coverage": "^3.4.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.29.1",
    "unplugin-auto-import": "^19.1.2",
    "unplugin-vue-components": "^28.5.0",
    "vite-plugin-eslint2": "^5.0.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "webpack": "^5.99.5",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-stats-viewer-plugin": "^1.1.1"
  },
  "license": "SEE LICENSE FILE"
}
