{
  "name": "s2maps-gpu",
  "type": "module",
  "version": "0.18.0",
  "description": "S2 Maps GPU - An open source, high-performance, and GPU-accelerated map engine for rendering large-scale, interactive maps.",
  "keywords": [
    "s2",
    "s2-geometry",
    "maps",
    "cartography",
    "gpu",
    "webgpu",
    "webgl2",
    "webgl",
    "digital",
    "mapping",
    "engine"
  ],
  "homepage": "https://opens2.com/s2maps-gpu",
  "bugs": {
    "url": "https://github.com/Open-S2/s2maps-gpu/issues"
  },
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/Open-S2"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/Open-S2/s2maps-gpu.git"
  },
  "scripts": {
    "build:dev": "nuxt build",
    "build:S2": "bun build:ESM && node ./configS2/buildWebpack.ts",
    "build:S2:flat": "bun run configS2/buildBunFlat.ts",
    "build:ESM": "rm -rf dist && tsc --project tsconfig.dist.json --jsx react-jsx",
    "build:frameworks": "vite build --config frameworks/vite.config.ts",
    "dev": "DEV=true nuxt dev --host --disable-telemetry",
    "docs": "typedoc --tsconfig tsconfig.typedoc.json",
    "lint:S2": "BUN_JSC_forceRAMSize=\"1073741\" bun --smol run eslint s2 --ext .ts,.tsx,.vue,.svelte --cache",
    "loc": "tokei ./s2 # && tokei ./rust",
    "server:playwright": "bun run ./tests-playwright/server.ts",
    "test:clean": "rm -rf .nyc_output coverage && mkdir .nyc_output && mkdir coverage",
    "test:S2": "bun test && mv coverage/lcov.info coverage/lcov-s2.info",
    "test:S2:watch": "bun test --watch",
    "test:playwright": "playwright test -c ./tests-playwright/playwright.config.ts && nyc report && mv coverage/lcov.info coverage/lcov-playwright.info",
    "test:playwright:ui": "playwright test --ui -c ./tests-playwright/playwright.config.ts",
    "test:merge": "lcov-result-merger 'coverage/lcov-*.info' > coverage/lcov.info",
    "test:all": "bun run test:clean && bun run test:S2 && bun run test:playwright && bun run test:merge",
    "prepare:toAdd": "bun run lint:S2 && bun run build:ESM && bun run build:frameworks"
  },
  "files": ["dist", "frameworks/dist"],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./frameworks": {
      "import": "./frameworks/dist/index.js",
      "types": "./frameworks/dist/index.d.ts"
    },
    "./plugins": {
      "import": "./dist/plugins.js",
      "types": "./dist/plugins.d.ts"
    },
    "./polyfills": {
      "import": "./dist/polyfills.js",
      "types": "./dist/polyfills.d.ts"
    }
  },
  "main": "./dist/index.js",
  "types": "./dist-types/index.d.ts",
  "dependencies": {
    "earclip": "^1.2.0",
    "open-vector-tile": "^1.12.0",
    "unicode-shaper": "0.7.0"
  },
  "devDependencies": {
    "@happy-dom/global-registrator": "^17.4.4",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@maplibre/maplibre-gl-style-spec": "^23.2.0",
    "@nuxt/eslint": "^1.3.0",
    "@playwright/experimental-ct-vue": "1.52.0",
    "@playwright/test": "1.52.0",
    "@rollup/pluginutils": "^5.1.4",
    "@sveltejs/adapter-auto": "^6.0.0",
    "@types/better-sqlite3": "^7.6.13",
    "@types/bun": "^1.2.10",
    "@types/mapbox-gl": "^3.4.1",
    "@types/maplibre-gl": "^1.14.0",
    "@types/react": "^19.1.2",
    "@types/webpack": "^5.28.5",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@webgpu/types": "^0.1.60",
    "ajv": "^8.17.1",
    "better-sqlite3": "^11.9.1",
    "compression-webpack-plugin": "^11.1.0",
    "coveralls": "^3.1.1",
    "css-loader": "^7.1.2",
    "css-minimizer-webpack-plugin": "^7.0.2",
    "esbuild": "^0.25.3",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^50.6.10",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-svelte": "^3.5.1",
    "eslint-plugin-tsdoc": "^0.4.0",
    "eslint-plugin-vue": "^10.0.0",
    "filesize": "^10.1.6",
    "find-cache-directory": "^6.0.0",
    "globals": "^16.0.0",
    "lcov-result-merger": "^5.0.1",
    "mini-css-extract-plugin": "^2.9.2",
    "nuxt": "^3.16.2",
    "nyc": "^17.1.0",
    "picocolors": "^1.1.1",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-vue": "^1.1.6",
    "raw-loader": "^4.0.2",
    "react": "^19.1.0",
    "s2-tilejson": "^1.13.0",
    "s2json-spec": "^1.11.0",
    "svelte": "^5.28.2",
    "svgo": "^3.3.2",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "typedoc": "^0.28.3",
    "typedoc-plugin-coverage": "^4.0.1",
    "typedoc-plugin-mdn-links": "^5.0.1",
    "typedoc-plugin-vue": "^1.5.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.31.0",
    "unplugin-auto-import": "^19.1.2",
    "unplugin-vue-components": "^28.5.0",
    "v8-to-istanbul": "^9.3.0",
    "vite-plugin-dts": "^4.5.3",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-eslint2": "^5.0.3",
    "vite-plugin-istanbul": "^7.0.0",
    "vite-plugin-svelte": "^3.0.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vue": "^3.5.13",
    "vue-eslint-parser": "^10.1.3",
    "vue-router": "^4.5.0",
    "webpack": "^5.99.6",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-stats-viewer-plugin": "^1.1.1"
  },
  "license": "SEE LICENSE FILE"
}
