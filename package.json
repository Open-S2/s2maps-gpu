{
  "name": "s2maps-gpu",
  "version": "0.13.3",
  "private": true,
  "description": "S2 Maps GPU",
  "keywords": [
    "s2",
    "s2-geometry",
    "maps",
    "cartography",
    "gpu",
    "webgpu",
    "webgl",
    "webgl2",
    "digital mapping engine"
  ],
  "homepage": "https://opens2.com/s2maps-gpu",
  "bugs": {
    "url": "https://github.com/Open-S2/s2maps-gpu/issues"
  },
  "funding": [
    {
      "type": "github",
      "url": "https://github.com/sponsors/Open-S2"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:Open-S2/s2maps-gpu.git"
  },
  "scripts": {
    "build:dev": "nuxt build",
    "build:S2": "node ./configS2/build.js",
    "build:S2:ES": "bun run ./configS2/buildESBuild.ts",
    "build:ESM": "tsc --project tsconfig.build.json",
    "build:types": "tsc --project tsconfig.build.json --emitDeclarationOnly --declaration --declarationDir dist-types",
    "tsc:version": "tsc --version",
    "dev": "nuxt dev --host",
    "lint:S2": "eslint s2 --ext .ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "prepare": "bun run lint:S2 && bun run build:ESM && bun run build:types"
  },
  "exports": {
    ".": "./dist/plugins/index.js"
  },
  "main": "./dist/plugins/index.js",
  "types": "./dist-types/index.d.ts",
  "dependencies": {
    "earclip": "^0.10.0",
    "s2-vector-tile": "^2.0.12",
    "unicode-shaper-zig": "0.2.0"
  },
  "devDependencies": {
    "@happy-dom/global-registrator": "^13.6.2",
    "@maplibre/maplibre-gl-style-spec": "^20.1.1",
    "@nuxtjs/eslint-module": "^4.1.0",
    "@rollup/pluginutils": "^5.1.0",
    "@types/better-sqlite3": "^7.6.9",
    "@types/bun": "^1.0.8",
    "@types/webpack-bundle-analyzer": "^4.7.0",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.1.0",
    "@webgpu/types": "^0.1.40",
    "ajv": "^8.12.0",
    "better-sqlite3": "^9.4.3",
    "compression-webpack-plugin": "^11.1.0",
    "css-loader": "^6.10.0",
    "css-minimizer-webpack-plugin": "^6.0.0",
    "eslint": "^8.57.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-vue": "^9.22.0",
    "filesize": "^10.1.0",
    "find-cache-dir": "^5.0.0",
    "mini-css-extract-plugin": "^2.8.1",
    "nuxt": "^3.10.3",
    "picocolors": "^1.0.0",
    "pixelmatch": "^5.3.0",
    "raw-loader": "^4.0.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "ts-standard": "*",
    "tsconfig-paths-webpack-plugin": "^4.1.0",
    "typescript": "latest",
    "vue": "^3.4.19",
    "vue-router": "^4.3.0",
    "webpack": "^5.90.3",
    "webpack-bundle-analyzer": "^4.10.1",
    "webpack-stats-viewer-plugin": "^1.1.1"
  },
  "license": "SEE LICENSE FILE"
}
