{
  "version": 1,
  "name": "countries-example",
  "center": [-122.4585607773497, 37.778443127730476],
  "zoom": -0.5,
  "minzoom": -0.5,
  "maxzoom": 1,
  "sources": {
    "countries": "s2maps://s2json/s2maps/countriesHD.s2json",
    "bathymetry": "s2maps://s2tiles/s2maps/bathymetry.s2tiles"
  },
  "fonts": {},
  "colorBlind": false,
  "layers": [
    {
      "name": "background",
      "type": "fill",
      "source": "mask",
      "opaque": true,
      "layout": {},
      "paint": {
        "color": "#fff"
      }
    },
    {
      "name": "water-fill",
      "source": "countries",
      "type": "fill",
      "invert": true,
      "layout": {},
      "paint": {
        "color": "#f5f9fb"
      }
    },
    {
      "name": "bath-fill",
      "source": "bathymetry",
      "layer": "bathymetry",
      "type": "fill",
      "filter": ["depth", "has", [200]],
      "layout": {},
      "paint": {
        "color": [
          "data-condition",
          ["depth", "==", 200],
          "#007bfe",
          "default",
          "#dbeafa"
        ]
      }
    },
    {
      "name": "country-fill",
      "source": "countries",
      "type": "fill",
      "filter": ["iso2", "has", ["US", "DE", "RU", "AU"]],
      "interactive": true,
      "cursor": "pointer",
      "layout": {},
      "paint": {
        "color": [
          "data-condition",
          ["iso2", "==", "US"],
          "#007bfe",
          ["iso2", "==", "DE"],
          "#dbeafa",
          ["iso2", "==", "RU"],
          "#f3f7fb",
          ["iso2", "==", "AU"],
          "#f0f6fa"
        ]
      }
    },
    {
      "name": "country-line",
      "source": "countries",
      "type": "line",
      "layout": {},
      "paint": {
        "color": "#bbd3de",
        "width": 1.25
      }
    }
  ]
}
