{
  "version": 1,
  "projection": "WM",
  "name": "webmercator background",
  "center": [0, 0],
  "zoom": 0.95,
  "minzoom": 0,
  "maxzoom": 6.9,
  "sources": {
    "planet": "/tiles/merc/osm"
  },
  "fonts": {},
  "layers": [
    {
      "name": "water-fill",
      "source": "planet",
      "layer": "water",
      "type": "fill",
      "invert": false,
      "opaque": false,
      "layout": {},
      "paint": {
        "color": "#b4c1c6"
      }
    },
    {
      "name": "country-line",
      "source": "planet",
      "layer": "boundary",
      "filter":  ["or", ["admin_level", "has", [3, 4, 5, 6, 7, 8, 9, 10]], ["maritime", "==", 0]],
      "type": "line",
      "layout": {},
      "paint": {
        "color": "#bbd3de",
        "width": 1.85
      }
    }
  ]
}
