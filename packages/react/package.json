{
  "name": "s2maps-gpu-react",
  "version": "1.0.1",
  "type": "module",
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/index.min.js",
  "browser": {
    "./sfc": "src/components/S2MapsGPU.vue"
  },
  "types": "dist/S2MapsGPU.d.ts",
  "files": ["dist"],
  "scripts": {
    "dev": "vite --host",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "build": "bun run build:umd & bun run build:esm & bun run build:unpkg",
    "build:types": "tsc -p tsconfig.types.json",
    "build:umd": "FORMAT=umd node ./esbuild.js",
    "build:esm": "FORMAT=esm node ./esbuild.js",
    "build:unpkg": "FORMAT=iife node ./esbuild.js",
    "prepare": "bun run lint && bun run build"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react-swc": "^3.5.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "s2maps-gpu": "^0.14.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.6"
  }
}
