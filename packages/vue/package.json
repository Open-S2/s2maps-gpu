{
  "name": "s2maps-gpu-vue",
  "version": "1.0.1",
  "type": "module",
  "main": "dist/index.umd.js",
  "module": "dist/index.esm.js",
  "unpkg": "dist/index.min.js",
  "browser": {
    "./sfc": "src/components/S2MapsGPU.vue"
  },
  "files": ["dist"],
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "vite --host",
    "lint": "eslint --ext .ts,.vue src",
    "build": "bun run build:umd & bun run build:esm & bun run build:unpkg",
    "build:new": "vue-cli-service build --dest dist --target lib --name s2maps-gpu-vue ./src/components/S2MapsGPU.vue",
    "build:types": "tsc -p tsconfig.types.json",
    "build:umd": "FORMAT=umd node ./esbuild.js",
    "build:esm": "FORMAT=esm node ./esbuild.js",
    "build:unpkg": "FORMAT=iife node ./esbuild.js",
    "prepare": "bun run lint && bun run build"
  },
  "dependencies": {
    "vue": "^3.4.21"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/cli-service": "^5.0.8",
    "esbuild": "^0.20.2",
    "esbuild-plugin-vue3": "^0.4.2",
    "s2maps-gpu": "^0.14.0",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vue-tsc": "^1.8.27"
  }
}
