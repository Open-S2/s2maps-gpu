{
  "version": 1,
  "name": "s2test",
  "center": [0, 0],
  "zoom": 0,
  "minzoom": -1,
  "maxzoom": 9.99,
  "sources": {
    "moon-poi": "http://localhost:8000/data/moon-poi",
    "moon": {
      "path": "http://192.168.0.110:8000/data/moon-v1",
      "type": "raster",
      "fileType": "webp",
      "minzoom": 0,
      "maxzoom": 10,
      "tileSize": 512
    }
  },
  "fonts": {
    "default": "https://s3.s2maps.io/fonts-v3/default",
    "roboto-regular": "https://s3.s2maps.io/fonts-v3/RobotoRegular",
    "roboto-medium": "https://s3.s2maps.io/fonts-v3/RobotoMedium"
  },
  "icons": {
  },
  "wallpaper": {
    "skybox": "https://s3.s2maps.io/backgrounds/milkyway",
    "loadingBackground": "rgb(9, 8, 17)",
    "size": 1024,
    "type": "jpg"
  },
  "colorBlind": false,
  "layers": [
    {
      "name": "background",
      "type": "fill",
      "source": "mask",
      "opaque": true,
      "layout": {},
      "paint": {
        "color": "rgb(64, 64, 64)"
      }
    },
    {
      "name": "moon-raster",
      "source": "moon",
      "opaque": true,
      "type": "raster"
    },
    {
      "name": "moon-craters",
      "source": "moon-poi",
      "layer": "craters",
      "type": "glyph",
      "minzoom": 0,
      "layout": {
        "text-family": "roboto-medium",
        "text-field": "?name",
        "text-anchor": "center",
        "text-offset": [0, 0],
        "text-padding": [1, 1]
      },
      "paint": {
        "text-size": [
          "input-range",
          "zoom",
          "expo",
          1.5,
          0,
          14,
          4,
          20
        ],
        "text-fill": "#ededed",
        "text-stroke": "rgba(35, 35, 35, 0.95)",
        "text-stroke-width": 0.85
      }
    },
    {
      "name": "moon-artificial",
      "source": "moon-poi",
      "layer": "artificial",
      "type": "glyph",
      "minzoom": 0,
      "overdraw": true,
      "interactive": true,
      "cursor": "pointer",
      "layout": {
        "icon-family": "moon-icons",
        "icon-field": "?icon",
        "icon-anchor": "bottom",
        "icon-offset": [0, -0.2],
        "icon-padding": [1, 1]
      },
      "paint": {
        "icon-size": [
          "input-range",
          "zoom",
          "lin",
          0,
          36,
          5,
          80
        ]
      }
    }
  ]
}
