{
  "version": 1,
  "name": "s2test",
  "center": [-111, 40],
  "zoom": 0,
  "minzoom": 0,
  "maxzoom": 20,
  "sources": {
    "fills": "http://localhost:8000/data/fills"
  },
  "fonts": {
    "default": "http://localhost:8000/fonts/default",
    "roboto-regular": "http://localhost:8000/fonts/roboto-regular",
    "roboto-medium": "http://localhost:8000/fonts/roboto-medium"
  },
  "shade": true,
  "wallpaper": {
    "background-color": "rgb(25, 28, 80)",
    "fade-1": "rgb(138, 204, 255)",
    "fade-2": "rgb(217, 255, 255)",
    "halo": "rgb(230, 255, 255)"
  },
  "sphere-background": {
    "background-color": [
      "input-range",
      "zoom",
      "expo",
      1.7,
      2.75,
      "rgb(244, 239, 226)",
      3,
      "rgb(245, 245, 245)",
      7.5,
      "rgb(236, 234, 231)",
      9.5,
      "rgb(229, 229, 229)",
      11.5,
      "rgb(238,240,239)",
      13,
      "rgb(238,240,239)",
      14,
      "rgb(248, 249, 250)"
    ]
  },
  "layers": [
    {
      "name": "landuse",
      "source": "fills",
      "layer": "landuse",
      "type": "fill",
      "filter": ["class", "has", ["park", "national_park", "protected_area",
        "aboriginal_lands", "grass", "grassland", "garden", "meadow",
        "wood", "swamp", "forest", "wetland", "zoo", "cemetery", "indianReservation",
        "sand", "beach", "desert", "aeroway", "military", "hospital",
        "university", "college", "school", "kindergarten", "parking", "glacier",
        "quarry", "industrial", "retail"]],
      "minzoom": 0,
      "layout": {},
      "paint": {
        "color": [
          "data-condition",
          ["class", "has", ["desert", "sand"]],
          "rgb(242, 236, 207)",
          ["class", "==", "aeroway"],
          "rgb(219, 222, 235)",
          ["class", "==", "military"],
          "rgb(226, 226, 224)",
          ["class", "==", "quarry"],
          "rgb(206, 208, 211)",
          ["class", "==", "retail"],
          "rgb(254, 247, 224)",
          ["class", "has", ["university", "college", "school", "kindergarten"]],
          "rgb(226, 233, 238)",
          ["class", "==", "parking"],
          "rgb(241, 243, 244)",
          ["class", "==", "hospital"],
          "rgb(252, 232, 230)",
          ["class", "has", ["park", "national_park", "protected_area"]],
          [
            "input-range",
            "zoom",
            "lin",
            3,
            "rgb(205, 236, 205)",
            4,
            "rgb(197, 232, 197)"
          ],
          ["class", "has", ["grass", "grassland", "garden", "meadow", "wood", "swamp", "forest", "zoo"]],
          "rgb(201, 238, 201)",
          ["class", "==", "cemetery"],
          "rgb(208, 236, 208)",
          ["class", "==", "beach"],
          "rgb(242, 236, 207)",
          ["class", "==", "glacier"],
          "rgb(249, 249, 249)",
          ["class", "==", "aboriginal_lands"],
          "rgb(240, 237, 235)",
          ["class", "==", "wetland"],
          "rgb(210, 233, 255)",
          "default",
          "rgba(0, 0, 0, 0)"
        ]
      }
    },
    {
      "name": "golf",
      "source": "fills",
      "layer": "landuse",
      "type": "fill",
      "filter": ["class", "==", "golf"],
      "minzoom": 0,
      "layout": {},
      "paint": {
        "color": [
          "data-condition",
          ["type", "has", ["course", "rough"]],
          "rgb(201, 238, 201)",
          ["type", "==", "fairway"],
          "rgb(218, 245, 205)",
          ["type", "==", "green"],
          "rgb(233, 251, 217)",
          ["type", "==", "bunker"],
          "rgb(255, 239, 195)",
          "default",
          "rgba(0, 0, 0, 0)"
        ]
      }
    },
    {
      "name": "water",
      "source": "fills",
      "filter": ["class", "!has", ["reef", "shoal"]],
      "layer": "water",
      "type": "fill",
      "minzoom": 0,
      "layout": {},
      "paint": {
        "color": "rgb(170, 218, 255)"
      }
    },
    {
      "name": "reef",
      "source": "fills",
      "filter": ["class", "has", ["reef", "shoal"]],
      "layer": "water",
      "type": "fill",
      "minzoom": 0,
      "layout": {},
      "paint": {
        "color": "rgb(210, 233, 255)"
      }
    },
    {
      "name": "rivers",
      "source": "fills",
      "layer": "water",
      "type": "line",
      "layout": {
        "cap": "butt",
        "join": "bevel"
      },
      "paint": {
        "color": "rgb(170, 218, 255)",
        "width": [
          "input-range",
          "zoom",
          "expo",
          1.5,
          9,
          0.25,
          12,
          2
        ]
      }
    },
    {
      "name": "transport_borders",
      "source": "fills",
      "layer": "transport",
      "filter": ["class", "has", ["primary", "primary_link", "secondary", "secondary_link", "tertiary", "tertiary_link", "street", "street_limited"]],
      "type": "line",
      "minzoom": 2,
      "layout": {
        "cap": "round",
        "join": "bevel"
      },
      "paint": {
        "color": [
          "data-condition",
          ["class", "has", ["primary", "primary_link", "secondary", "secondary_link", "tertiary", "tertiary_link", "street", "street_limited"]],
          "rgb(218, 220, 224)",
          "default",
          "rgba(0, 0, 0, 0)"
        ],
        "width": [
          "data-condition",
          ["class", "has", ["primary", "primary_link", "secondary", "secondary_link", "tertiary", "tertiary_link"]],
          [
            "input-range",
            "zoom",
            "lin",
            4,
            0.85,
            6.5,
            0.85,
            7,
            2,
            10,
            2.5,
            13,
            7
          ],
          ["or", ["class", "==", "street"], ["class", "==", "street_limited"]],
          [
            "input-range",
            "zoom",
            "lin",
            10,
            0.5,
            15,
            4.85
          ],
          "default",
          1.85
        ]
      }
    },
    {
      "name": "transport",
      "source": "fills",
      "layer": "transport",
      "filter": ["or",
        ["class", "has", ["primary", "primary_link", "secondary", "secondary_link", "tertiary", "tertiary_link", "street", "street_limited", "ferry"]],
        ["and", ["class", "==", "railway"], ["type", "==", "rail"]],
        ["and", ["class", "==", "path"], ["type", "==", "path"]]
      ],
      "type": "line",
      "minzoom": 2,
      "layout": {
        "cap": "round",
        "join": "bevel"
      },
      "paint": {
        "color": [
          "data-condition",
          ["class", "==", "ferry"],
          "#9fccf9",
          ["class", "==", "path"],
          "rgb(136, 190, 150)",
          ["class", "==", "railway"],
          "rgb(214, 217, 220)",
          ["class", "has", ["primary", "primary_link", "secondary", "secondary_link", "tertiary", "tertiary_link", "street", "street_limited"]],
          "rgb(255, 255, 255)",
          "default",
          "rgba(0, 0, 0, 0)"
        ],
        "width": [
          "data-condition",
          ["class", "has", ["primary", "primary_link", "secondary", "secondary_link", "tertiary", "tertiary_link"]],
          [
            "input-range",
            "zoom",
            "lin",
            7,
            0.85,
            10,
            1.5,
            13,
            6
          ],
          ["or", ["class", "==", "street"], ["class", "==", "street_limited"]],
          [
            "input-range",
            "zoom",
            "lin",
            10,
            0.5,
            15,
            4
          ],
          ["class", "==", "path"],
          1,
          "default",
          1
        ]
      }
    },
    {
      "name": "motor_trunk_borders",
      "source": "fills",
      "layer": "transport",
      "filter": ["class", "has", ["motorway", "motorway_link", "trunk", "trunk_link"]],
      "type": "line",
      "minzoom": 2,
      "layout": {
        "cap": "butt",
        "join": "bevel"
      },
      "paint": {
        "color": "rgb(246, 207, 101)",
        "width": [
          "input-range",
          "zoom",
          "lin",
          7,
          1.85,
          11,
          3,
          15,
          10
        ]
      }
    },
    {
      "name": "motor_trunk",
      "source": "fills",
      "layer": "transport",
      "filter": ["class", "has", ["motorway", "motorway_link", "trunk", "trunk_link"]],
      "type": "line",
      "minzoom": 2,
      "layout": {
        "cap": "round",
        "join": "bevel"
      },
      "paint": {
        "color": "rgb(255, 235, 161)",
        "width": [
          "input-range",
          "zoom",
          "lin",
          7,
          0.5,
          11,
          1.5,
          15,
          9
        ]
      }
    },
    {
      "name": "boundaries",
      "source": "fills",
      "layer": "boundary",
      "filter": ["and", ["class", "has", ["Country", "State"]], ["maritime", "==", false]],
      "type": "line",
      "minzoom": 0,
      "layout": {
        "cap": "butt",
        "join": "bevel"
      },
      "paint": {
        "color": [
          "data-condition",
          ["class", "==", "State"],
          "rgba(160, 160, 160, 0.7)",
          "default",
          "rgb(150, 150, 150)"
        ],
        "width": [
          "data-condition",
          ["class", "==", "State"],
          0.85,
          "default",
          [
            "input-range",
            "zoom",
            "lin",
            1,
            0.5,
            2,
            0.95
          ]
        ]
      }
    },
    {
      "name": "equator",
      "source": "fills",
      "layer": "equator",
      "type": "line",
      "minzoom": 0,
      "maxzoom": 10,
      "layout": {
        "cap": "butt",
        "join": "bevel"
      },
      "paint": {
        "color": "rgba(125, 102, 97, 0.8)",
        "width": [
          "input-range",
          "zoom",
          "lin",
          0.99,
          1.5,
          1,
          3
        ]
      }
    },
    {
      "name": "buildings",
      "source": "fills",
      "layer": "building",
      "type": "fill",
      "layout": {},
      "paint": {
        "color": "rgb(235, 235, 235)"
      }
    },
    {
      "name": "places",
      "source": "fills",
      "filter": ["class", "has", ["Country", "State"]],
      "layer": "place",
      "type": "text",
      "layout": {
        "family": "roboto-medium",
        "field": "?abbr,?name",
        "anchor": "center",
        "offset": [0, 0],
        "padding": [2, 2]
      },
      "paint": {
        "size": [
          "data-condition",
          ["class", "==", "State"],
          [
            "input-range",
            "zoom",
            "step",
            1,
            20,
            2,
            18
          ],
          ["class", "==", "Country"],
          [
            "input-range",
            "zoom",
            "step",
            0,
            26,
            2,
            20
          ],
          "default",
          14
        ],
        "fill": [
          "data-condition",
          ["class", "==", "State"],
          "rgb(135, 135, 135)",
          ["class", "==", "Country"],
          "rgb(38, 38, 38)",
          "default",
          "rgba(0, 0, 0, 0)"
        ],
        "stroke": "rgb(250, 250, 250)",
        "strokeWidth": 1
      }
    }
  ]
}
