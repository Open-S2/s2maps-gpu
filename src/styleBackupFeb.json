{
  "version": 1,
  "name": "s2test",
  "center": [0, 0],
  "zoom": 0,
  "minzoom": 0,
  "maxzoom": 20,
  "sources": {
    "fills": "http://localhost:8000/data/fills",
    "utah": "http://localhost:8000/json/utah.s2json"
  },
  "shade": true,
  "wallpaper": {
    "background-color": "rgb(25, 28, 80)",
    "fade-1": "rgb(138, 204, 255)",
    "fade-2": "rgb(217, 255, 255)",
    "halo": "rgb(230, 255, 255)"
  },
  "sphere-background": {
    "background-color": [
      "input-range",
      "zoom",
      "expo",
      1.7,
      2,
      "rgb(244, 239, 226)",
      3,
      "rgb(245, 245, 245)",
      6,
      "rgb(245, 245, 245)",
      8.5,
      "rgb(236, 234, 231)",
      11,
      "rgb(236, 234, 231)",
      12,
      "rgb(242,241,240)"
    ]
  },
  "layers": [
    {
      "id": "parks",
      "source": "fills",
      "layer": "landuse",
      "type": "fill",
      "filter": ["and", ["class", "has", ["park", "national_park", "protected_area", "aboriginal_lands", "golf", "grass", "grassland", "garden", "meadow", "wood", "swamp", "forest", "wetland", "zoo", "cemetery", "indianReservation", "sand", "beach", "desert", "aeroway", "military", "hospital", "university", "college", "school", "kindergarten", "parking", "glacier", "quarry", "industrial", "retail"]]],
      "minzoom": 0,
      "layout": {},
      "paint": {
        "color": [
          "data-condition",
          ["class", "has", ["desert", "sand", "beach"]],
          "rgb(242, 236, 207)",
          ["class", "==", "aeroway"],
          "rgb(219, 222, 235)",
          ["class", "==", "military"],
          "rgb(226, 226, 224)",
          ["class", "==", "quarry"],
          "rgb(206, 208, 211)",
          ["class", "==", "retail"],
          "rgb(254, 247, 224)",
          ["class", "has", ["university", "college", "school", "kindergarten"]],
          "rgb(226, 233, 238)",
          ["class", "==", "parking"],
          "rgb(241, 243, 244)",
          ["class", "==", "hospital"],
          "rgb(249, 220, 218)",
          ["class", "has", ["park", "national_park", "protected_area"]],
          [
            "input-range",
            "zoom",
            "lin",
            3,
            "rgb(205, 236, 205)",
            4,
            "rgb(197, 232, 197)"
          ],
          ["class", "has", ["golf", "grass", "grassland", "garden", "meadow", "wood", "swamp", "forest", "zoo"]],
          "rgb(201, 238, 201)",
          ["class", "==", "cemetery"],
          "rgb(208, 236, 208)",
          ["class", "==", "industrial"],
          "rgb(232, 232, 232)",
          ["class", "==", "glacier"],
          "rgb(249, 249, 249)",
          ["class", "==", "aboriginal_lands"],
          "rgb(240, 237, 235)",
          ["class", "==", "wetland"],
          "rgb(210, 233, 255)",
          "default",
          "rgba(0, 0, 0, 0)"
        ]
      }
    },
    {
      "id": "water",
      "source": "fills",
      "filter": ["and", ["class", "!has", ["reef", "shoal"]]],
      "layer": "water",
      "type": "fill",
      "minzoom": 0,
      "layout": {},
      "paint": {
        "color": "rgb(170, 218, 255)"
      }
    },
    {
      "id": "reef",
      "source": "fills",
      "filter": ["and", ["class", "has", ["reef", "shoal"]]],
      "layer": "water",
      "type": "fill",
      "minzoom": 0,
      "layout": {},
      "paint": {
        "color": "rgb(210, 233, 255)"
      }
    },
    {
      "id": "equator",
      "source": "fills",
      "layer": "equator",
      "type": "line",
      "minzoom": 0,
      "maxzoom": 10,
      "layout": {
        "cap": "butt",
        "join": "bevel"
      },
      "paint": {
        "color": "rgba(125, 102, 97, 0.8)",
        "width": [
          "input-range",
          "zoom",
          "lin",
          0.99,
          1.5,
          1,
          3
        ]
      }
    },
    {
      "id": "state",
      "source": "utah",
      "layer": "default",
      "type": "line",
      "minzoom": 0,
      "maxzoom": 10,
      "layout": {
        "cap": "butt",
        "join": "bevel"
      },
      "paint": {
        "color": "rgb(216, 85, 77)",
        "width": 0.85
      }
    }
  ]
}
